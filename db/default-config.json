{
  "laragon_path"     : "E:\\localhost",
  "document_root"    : "${LARAGON_PATH}\/www",
  "host_file_prefix" : "${LARAGON_PATH}\/etc\/ssl",
  "ssl_storage_path" : "",
  "platform"         : "windows",
  "hosts_file_entry" : "on",
  "hosts_file"       : "C:\\Windows\\System32\\drivers\\etc\\hosts",
  "library"          : {
	"mkcert_path" : "${LARAGON_TOOLKIT_PATH}\/library\/mkcert.exe"
  },
  "apache"           : {
	"config"        : "${LARAGON_PATH}\/etc\/apache2\/",
	"sites_enabled" : "${APACHE_PATH}\/sites-enabled\/",
	"alias"         : "${APACHE_PATH}\/alias\/",
	"force_ssl"      : true,
	"ports"         : {
	  "http"  : "80",
	  "https" : "443"
	}
  },
  "nginx"            : {
	"config"        : "${LARAGON_PATH}\/etc\/nginx\/",
	"sites_enabled" : "${NGINX_PATH}\/sites-enabled\/",
	"alias"         : "${NGINX_PATH}\/alias\/",
	"ports"         : {
	  "http"  : "81",
	  "https" : "444"
	}
  },
  "mysql"            : {
	"username" : "root",
	"password" : "",
	"host"     : "localhost"
  },
  "wp_clone"         : {
	"template"    : "${GLOBAL_DOCUMENT_ROOT}/",
	"template_db" : "wp_template"
  }
}